{
  "env": {
      "browser": true,
      "node": true
  },
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
      "arrow-parens": 0,
      "comma-dangle": 0,
      "import/first": 0,
      "jsx-quotes": 0,
      "no-console": 0,
      "no-param-reassign": ["error", { "props": false }],
      "no-unused-vars": 1,
      "react/jsx-filename-extension": 0,
      "semi": 0,
      "strict": 0,
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [ "Link" ],
          "specialLink": [ "to" ],
          "aspects": [ "noHref", "invalidHref" ]
        }
      ]
  }
}
